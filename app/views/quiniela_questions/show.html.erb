<!-- begin row -->
<div class="row">
  <!-- begin col-md-12 -->
  <div class="col-md-12">
    <div class="invoice">
      <div class="invoice-content">
        <div class="table-responsive">
         <div class="row">
			<div class="col-md-4">
				<h4> <%= t("activerecord.attributes.quiniela_question.question") %>: </h4>
				<h5><%= @quiniela_question.question.title %></h5>
			 </div>
			 <div class="col-md-4">
				 <h4> <%= t("activerecord.attributes.quiniela_question.quiniela") %>: </h4>
				 <h5><%= @quiniela_question.quiniela.description %></h5>
			 </div>
			 <div class="col-md-4">
				 <h4> <%= t("activerecord.attributes.quiniela_question.value") %>: </h4>
				 <h5><%= @quiniela_question.value %></h5>
			 </div>
			</div>
         <div class="row">
			<div class="col-md-4">
			 <h4> <%= t("activerecord.attributes.quiniela_question.created_at") %>: </h4>
             <h5><%= @quiniela_question.created_at %></h5>
			 </div>
			</div>
                 
        </div>
      </div>
      <div class="invoice-footer text-muted"></div>
    </div>
  </div>
  <!-- end col-md-12 -->
</div>
<!-- end row -->

<!-- begin row -->
<div class="row">
  <div class="col-md-12">
    <% if can? :edit, @quiniela_question %>
      <%= link_to edit_quiniela_question_path(@quiniela_question), :class => "btn btn-default" do %>
        <i class="icon-pencil"></i>
        <%= t("actions.edit") %>
      <% end %>
    <% end %>
    <% if can? :delete, @quiniela_question %>
      <%= link_to @quiniela_question, method: :delete, data: { confirm: t("are_you_sure?", model: t("activerecord.models.quiniela_question").downcase) }, :class => "btn btn-danger" do %>
        <i class="icon-trash"></i>
        <%= t("actions.delete") %>
      <% end %>
    <% end %>
    <% if can? :index, QuinielaQuestion  %>
      <%= link_to quiniela_questions_path, :class => "btn btn-default" do %>
        <i class="icon-chevron-left"></i>
        <%= t("actions.back") %>
      <% end %>
    <% end %>
  </div>
  <!-- end col-12 -->
</div>
<!-- end row -->