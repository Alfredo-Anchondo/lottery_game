<div class="row">
  <!-- begin col-12 -->
  <div class="col-md-12">
    <!-- begin panel -->
    <div class="panel panel-primary">
      <div class="panel-heading">
        <div class="row">
          <h3 style="margin-left:15px"><%= t("activerecord.models.survivor_week_survivors") %></h3>
        </div>
      </div>
      <div class="panel-body">
        <div class="table-responsive">
          <table id="data-table" class="table table-bordered table-hover table-striped nowrap" width="100%">
            <thead>
              <tr>
                                  <th><%= t("activerecord.attributes.survivor_week_survivor.survivor_id") %></th>
                                  <th><%= t("activerecord.attributes.survivor_week_survivor.survivor_week_game_id") %></th>
                                <th style="width: 155px"></th>
              </tr>
            </thead>

            <tbody>
              <% @survivor_week_survivors.each do |survivor_week_survivor| %>
              <tr>
                                  <td><%= Survivor.find(survivor_week_survivor.survivor_id).name %></td>
                                  <td>Semana <%= SurvivorWeekGame.find(survivor_week_survivor.survivor_week_game_id).week %></td>
                                <td class="action-link">
                  <% if can? :show,survivor_week_survivor %>
                    <%= link_to survivor_week_survivor, :class => "btn btn-default", :title => t("actions.show") do %>
                      <i class="fa fa-eye"></i>
                    <% end %>
                  <% end %>
                  <% if can? :edit, survivor_week_survivor %>
                    <%= link_to edit_survivor_week_survivor_path(survivor_week_survivor), :class => "btn btn-info", :title => t("actions.edit") do %>
                      <i class="fa fa-pencil"></i>
                    <% end %>
                  <% end %>
                  <% if can? :delete, survivor_week_survivor %>
                    <%= link_to survivor_week_survivor, :method => :delete, :data => { :confirm => t("are_you_sure?", model: t("activerecord.models.survivor_week_survivor").downcase) }, :class => "btn btn-danger", :title => t("actions.delete") do %>
                      <i class="fa fa-trash"></i>
                    <% end %>
                  <% end %>
                </td>
              </tr>
              <% end %>
            </tbody>
          </table>
        </div>
        <div class="text-center">
        
        </div>
      </div>
      <!-- end panel-body -->
    </div>
    <!-- end panel -->
  </div>
  <!-- end col-12 -->
</div>
<!-- end row -->

<!-- begin row -->
<div class="row">
  <div class="col-md-12">
    <% if can? :new, SurvivorWeekSurvivor  %>
      <%= link_to new_survivor_week_survivor_path, :class => "btn btn-default" do %>
        <i class="icon-plus-sign-alt"></i>
        <%= t("actions.new") %>
      <% end %>
    <% end %>
  </div>
  <!-- end col-12 -->
</div>
<!-- end row -->