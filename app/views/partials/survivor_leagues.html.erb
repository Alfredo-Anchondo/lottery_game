			     var password = $('#'+password1+'').val();
				 
				 if($('.'+input).val() == password){
					  window.location = '/partials/set_survivor_page/'+survivor+''
				 }else{
					 $(this).prop('disabled',false);
				 }
			 })
		  
		  $('.send_mail').click(function(){
			  
			var survivor = $(this).data('survivor');
			 var owner = $(this).data('id');
			   $.ajax({
            method: "GET",
            url: "/partials/access_mail",
            format: "json",
			data: ({owner: owner, survivor: survivor})	   
               }).done(function(data) {
				   alert('Se a notificado a nuestros administradores, le notificaremos al creador de la liga y te responderemos lo mas pronto posible.');
