<!-- begin row -->
<div class="row">
  <!-- begin col-md-12 -->
  <div class="col-md-12">
    <div class="invoice">
      <div class="invoice-content">
        <div class="table-responsive">
          <table class="table table-invoice">
            <thead>
              <tr>
                <td colspan="2">
                  <h4><%= t("details_of", model: t("activerecord.models.enrachate_user").downcase) %></h4>
                </td>
              </tr>
            </thead>
            <tbody>
                              <tr>
                  <td>
                    <%= t("activerecord.attributes.enrachate_user.question_enrachate") %>: <br />
                    <small><%= @enrachate_user.question_enrachate %></small>
                  </td>
                </tr>
                              <tr>
                  <td>
                    <%= t("activerecord.attributes.enrachate_user.tira_enrachate") %>: <br />
                    <small><%= @enrachate_user.tira_enrachate %></small>
                  </td>
                </tr>
                              <tr>
                  <td>
                    <%= t("activerecord.attributes.enrachate_user.answer") %>: <br />
                    <small><%= @enrachate_user.answer %></small>
                  </td>
                </tr>
                              <tr>
                  <td>
                    <%= t("activerecord.attributes.enrachate_user.user") %>: <br />
                    <small><%= @enrachate_user.user %></small>
                  </td>
                </tr>
                              <tr>
                  <td>
                    <%= t("activerecord.attributes.enrachate_user.status") %>: <br />
                    <small><%= @enrachate_user.status %></small>
                  </td>
                </tr>
                              <tr>
                  <td>
                    <%= t("activerecord.attributes.enrachate_user.purchase_date") %>: <br />
                    <small><%= @enrachate_user.purchase_date %></small>
                  </td>
                </tr>
                              <tr>
                  <td>
                    <%= t("activerecord.attributes.enrachate_user.created_at") %>: <br />
                    <small><%= @enrachate_user.created_at %></small>
                  </td>
                </tr>
            </tbody>
          </table>
        </div>
      </div>
      <div class="invoice-footer text-muted"></div>
    </div>
  </div>
  <!-- end col-md-12 -->
</div>
<!-- end row -->

<!-- begin row -->
<div class="row">
  <div class="col-md-12">
    <% if can? :edit, @enrachate_user %>
      <%= link_to edit_enrachate_user_path(@enrachate_user), :class => "btn btn-default" do %>
        <i class="icon-pencil"></i>
        <%= t("actions.edit") %>
      <% end %>
    <% end %>
    <% if can? :delete, @enrachate_user %>
      <%= link_to @enrachate_user, method: :delete, data: { confirm: t("are_you_sure?", model: t("activerecord.models.enrachate_user").downcase) }, :class => "btn btn-danger" do %>
        <i class="icon-trash"></i>
        <%= t("actions.delete") %>
      <% end %>
    <% end %>
    <% if can? :index, EnrachateUser  %>
      <%= link_to enrachate_users_path, :class => "btn btn-default" do %>
        <i class="icon-chevron-left"></i>
        <%= t("actions.back") %>
      <% end %>
    <% end %>
  </div>
  <!-- end col-12 -->
</div>
<!-- end row -->